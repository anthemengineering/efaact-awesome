{
  "name": "eFAACT Awesomizer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Takes the eFAACT time entry table and biggerizes it.",
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },  
  "permissions": [
    "tabs",
    "https://timeandexpense.myefaactweb.com/application/timecard2013.aspx"
  ],
  "page_action": {
    "default_icon" : "icon.png",
    "default_title" : "Make eFAACT more awesome?"
  }
}